        },
        "tags": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "title": "Tags"
        },
        "responders": {
          "type": "array",
          "title": "Responders",
          "description": "Responders to the alert"
        },
        "priority": {
          "type": "string",
          "title": "Priority"
        },
        "createdAt": {
          "title": "Create At",
          "type": "string",
          "format": "date-time"
        },
        "updatedAt": {
          "title": "Updated At",
          "type": "string",
          "format": "date-time"
        }
